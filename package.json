{
  "name": "squares3",
  "version": "0.1.0",
  "description": "An HTML5 version of the Squares 2 flash game by Gavin Shapiro",
  "author": "BranacleBoy <https://github.com/bmcminn>",
  "license": "MIT",
  "devDependencies": {
    "debug": "^4.3.1",
    "nollup": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss-hot": "^0.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "autobuild": "rollup -c -w",
    "build": "rollup -c",
    "build:prod": "set NODE_ENV=production && rollup -c",
    "clean": "rm -r build/bundle.js*",
    "dev": "run-p start:dev autobuild",
    "start": "sirv build --single",
    "start:dev": "sirv build --single --dev"
  }
}
